QMainWindow, QDialog {
    background: #2C2C2C;
}

QLabel {
    color: #673AB7;
    font-weight: bold;
    font-size: 25px;
}

QPushButton {
    background: #673AB7;
    border-width: 1px;
    border-style: solid;
    border-radius: 10px;
    padding: 5px;
}

QPushButton:hover {
    background: #009999;
}

QPushButton:pressed {
    background: #006666;
}

QLineEdit{
    background-color: #444444;
    border-width: 1px;
    border-style: solid;
    border-radius: 10px;
    padding: 5px;
}

QListWidget{
    background: #444444;
    border-width: 1px;
    border-style: solid;
    border-radius: 10px;
    padding: 5px;
}

QRadioButton::indicator {
    width: 10px;
    height: 10px;
    border: 1px solid;
    border-radius: 15px;
    background-color: #666666;
}

QRadioButton::indicator:checked {
    border-color: #000000;
    background-color: #333333;
}

QRadioButton::indicator:hover {
    border-color: #333333;
}